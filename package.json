{
  "name": "timer",
	"displayName": "Timer",
	"description": "An extension to track down your time spent coding.",
	"version": "1.0.0",
	"publisher": "dharmey",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dharmey747/vscode-timer.git"
	},
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
        "onCommand:timer.resetTimer"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "timer.resetTimer",
            "title": "Reset Timer"

		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.59.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }

}
